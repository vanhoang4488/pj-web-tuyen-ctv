String getRandomStr(int size) {
	String alphabet = ((&apos;a&apos;..&apos;z&apos;) + (&apos;A&apos;..&apos;Z&apos;) + (&apos;0&apos;..&apos;9&apos;)).join();
	return new Random().with{
		(1..size).collect{
			alphabet[nextInt(alphabet.length())]	
		}.join();
	};
}

void getParameters(){
	String loginName = &quot;hoangnv&quot;;
	String timestamp = &quot;&quot; + System.currentTimeMillis();
	String randomStr = getRandomStr(32);

	
	Map&lt;String, String&gt; params = new TreeMap&lt;&gt;();
	params.put(&quot;loginName&quot;, loginName);
	params.put(&quot;randomStr&quot;, randomStr);
	params.put(&quot;timestamp&quot;, timestamp);
	StringBuilder paramsBuilder = new StringBuilder();
	params.forEach((k, v) -&gt; {
			paramsBuilder.append(k).append(&quot;=&quot;).append(v).append(&quot;&amp;&quot;);	
		}
	);

	String sign = paramsBuilder.md5();

	vars.put(&quot;loginName&quot;, URLEncoder.encode(loginName, &quot;utf-8&quot;));
	vars.put(&quot;randomStr&quot;, URLEncoder.encode(randomStr, &quot;utf-8&quot;));
	vars.put(&quot;timestamp&quot;, URLEncoder.encode(timestamp, &quot;utf-8&quot;));
	vars.put(&quot;sign&quot;, URLEncoder.encode(sign, &quot;utf-8&quot;));
}

getParameters();
</stringProp>
